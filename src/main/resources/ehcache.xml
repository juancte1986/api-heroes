<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true">
	<cache name="heroesById" maxElementsInMemory="400"
		eternal="false" overflowToDisk="false" timeToLiveSeconds="3600"
		timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU"
		transactionalMode="off">
	</cache>
     <cache name="allHeroes" maxElementsInMemory="400"
		eternal="false" overflowToDisk="false" timeToLiveSeconds="3600"
		timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU"
		transactionalMode="off">
	</cache>
	<cache name="AllHeroesByName" maxElementsInMemory="400" eternal="false"
		overflowToDisk="false" timeToLiveSeconds="3600" timeToIdleSeconds="0"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
	</cache>
</ehcache>